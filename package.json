{
  "name": "@formulajs/formulajs",
  "version": "3.0.0",
  "description": "JavaScript implementation of most Microsoft Excel formula functions",
  "author": "Formulajs",
  "homepage": "https://github.com/formulajs/formulajs#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/formulajs/formulajs.git"
  },
  "bugs": {
    "url": "https://github.com/formulajs/formulajs/issues"
  },
  "license": "MIT",
  "type": "module",
  "main": "./lib/cjs",
  "module": "./lib/esm",
  "unpkg": "./lib/browser/formula.min.js",
  "jsdelivr": "./lib/browser/formula.min.js",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.cjs",
      "import": "./lib/esm/index.mjs"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "npm run lint && rollup -c",
    "coverage": "c8 mocha --reporter min",
    "lint": "eslint ./",
    "stats": "node ./bin/implementation-stats.js",
    "test": "mocha --reporter min",
    "test-browser": "mocha --reporter mochawesome",
    "watch": "mocha --watch --parallel --reporter min"
  },
  "dependencies": {
    "bessel": "^1.0.2",
    "jstat": "^1.9.5"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "c8": "^7.11.0",
    "cli-table3": "^0.6.1",
    "eslint": "^8.7.0",
    "mocha": "^9.2.0",
    "mochawesome": "^7.0.1",
    "rollup": "^2.66.1",
    "rollup-plugin-cleaner": "^1.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "should": "^13.2.3"
  }
}
